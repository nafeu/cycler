{
  "name": "cycler",
  "version": "0.0.1",
  "description": "Multimedia crossposting tools.",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "prebuild": "rm -rf dist build && mkdir dist",
    "build": "react-scripts build && cp -rf ./build ./dist && ./node_modules/.bin/babel -d ./dist ./server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "react-scripts start",
    "server-watch": "./node_modules/.bin/nodemon --ignore 'src/' --exec ./node_modules/.bin/babel-node ./server",
    "server": "./node_modules/.bin/babel-node ./server",
    "dev": "npm-run-all --parallel client server",
    "deploy": "git push heroku main"
  },
  "keywords": [
    "youtube",
    "instagram",
    "upload",
    "automation"
  ],
  "author": "Nafeu Nasir <nafeu.nasir@gmail.com> (http://nafeu.com/)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^5.1.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "firebase": "^9.6.7",
    "get-video-dimensions": "^1.0.0",
    "googleapis": "^95.0.0",
    "helmet": "^3.18.0",
    "instagram-private-api": "^1.45.3",
    "joi": "17.4.0",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.4.0",
    "react-select": "^5.2.2",
    "react-use": "^17.3.2",
    "reactstrap": "^8.0.0",
    "regenerator-runtime": "^0.13.7",
    "typeface-roboto-mono": "0.0.75"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "nodemon": "^2.0.4",
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "15.6.0"
  },
  "proxy": "http://localhost:8000"
}
