{
  "name": "cycler",
  "version": "0.0.1",
  "description": "Multimedia crossposting tools.",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "prebuild": "rm -rf dist build && mkdir dist",
    "build": "react-scripts --openssl-legacy-provider build && cp -rf ./build ./dist && ./node_modules/.bin/babel -d ./dist ./server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "react-scripts --openssl-legacy-provider start",
    "server": "./node_modules/.bin/nodemon --ignore 'src/' --exec ./node_modules/.bin/babel-node ./server",
    "dev": "npm-run-all --parallel client server",
    "deploy": "git push heroku main"
  },
  "keywords": [
    "youtube",
    "instagram",
    "upload",
    "automation"
  ],
  "author": "Nafeu Nasir <nafeu.nasir@gmail.com> (http://nafeu.com/)",
  "license": "MIT",
  "dependencies": {
    "axios": "0.21.0",
    "bootstrap": "^5.1.3",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "^3.18.0",
    "joi": "17.3.0",
    "lodash": "4.17.20",
    "morgan": "1.10.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "^5.0.0",
    "reactstrap": "^9.0.1",
    "regenerator-runtime": "0.13.7",
    "typeface-roboto-mono": "0.0.75"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/node": "7.12.6",
    "@babel/preset-env": "7.12.1",
    "nodemon": "2.0.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "17.5.0"
  },
  "proxy": "http://localhost:8000",
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  }
}
